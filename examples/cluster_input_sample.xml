<!-- Only constraint is optional-->
<!-- Miner can be single or cluster-->
<input>
    <miner>cluster</miner>
    <cluster>
        <input_t>200</input_t>
        <cluster_time>4</cluster_time>
        <cluster_prd>2</cluster_prd>
    </cluster>
    <prds>
        <prd>p1:26::</prd>
        <prd>p2:26::</prd>
    </prds>
    <bltls> <!-- bltls are connected with conjunction-->
        <bltl>p1</bltl>
        <bltl>F[k1]G[k2]p2</bltl>
    </bltls>
    <constraints></constraints>
    <weights>
        <weight>p1:0.2</weight>
        <weight>p2:0.2</weight>
        <weight>k1:0.2</weight>
        <weight>k2:0.2</weight>
    </weights>
    <config>
        <N_TRIES>200</N_TRIES>
        <ITERS_FIXED_T>1000</ITERS_FIXED_T>
        <STEP_SIZE>1.0</STEP_SIZE>
        <K>1.0</K>
        <T_INITIAL>0.008</T_INITIAL>
        <MU_T>1.003</MU_T>
        <T_MIN>2.0e-6</T_MIN>
    </config>
</input>

