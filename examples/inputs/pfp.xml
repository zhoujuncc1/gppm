<!-- Only constraint is optional-->
<!-- Miner can be single or cluster-->
<input>
    <miner>single</miner>
    <prds>
        <prd>p1:5::</prd>
        <prd>p2:5::</prd>
    </prds>
    <bltls>
        <bltl>p1 & F[k1]p2</bltl>
    </bltls>
    <constraints>
    <constraint>p2>p1</constraint>
    </constraints>
    <weights>
        <weight>k1:0.1</weight>
        <weight>p2:0.5</weight>
    </weights>
    <config>
        <N_TRIES>100</N_TRIES>
        <ITERS_FIXED_T>200</ITERS_FIXED_T>
        <STEP_SIZE>1.0</STEP_SIZE>
        <K>1.0</K>
        <T_INITIAL>0.008</T_INITIAL>
        <MU_T>1.003</MU_T>
        <T_MIN>0.001</T_MIN>
    </config>
</input>

